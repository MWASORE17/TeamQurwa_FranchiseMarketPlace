<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:clipToPadding="false"
    android:background="@color/colorwhitegold"
    android:id="@+id/franchisedetailprofile_container"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@color/dot_light_screen3">
                <ImageView
                    android:id="@+id/franchisedetailprofile_bookmark"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="35dp"
                    android:layout_gravity="center"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/tagcircle"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@color/dot_light_screen1">
                <ImageView
                    android:id="@+id/franchisedetailprofile_email"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="35dp"
                    android:layout_gravity="center"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/messagecircle"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@color/dot_light_screen4">
                <ImageView
                    android:id="@+id/franchisedetailprofile_call"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="35dp"
                    android:layout_gravity="center"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:src="@drawable/phonecircle"/>
            </LinearLayout>
        </LinearLayout>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="6dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="20dp"
                android:background="@color/white"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/kantor"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:layout_gravity="center"
                        android:id="@+id/homedetailprofileName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:textColor="@color/black"
                        android:text="Pizza Nut" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/type"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileJenis"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="17sp"
                        android:textColor="@color/black"
                        android:text="Lokal" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="65dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/category"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileKategori"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="17sp"
                        android:textColor="@color/black"
                        android:text="Makanan dan Minuman" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/calendar"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileBerdiriSejak"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="2017" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="65dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/investment"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileInvestasi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Rp. 1.000.000.000" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/alamat"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileAlamat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Jl. Thamrin No. 40 A Medan" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="65dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/earth"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileLokasi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Indonesia, North Sumatera" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/phonenumber"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileTelepon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="0812-3456-7890" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="65dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/email1"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileEmail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="pizzawillkickyournuts@pizza.net.id" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/website"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/homedetailprofileWebsite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="www.pizzawillkickyournuts.com" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="65dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:textColor="@color/darkgray"
                        android:layout_marginBottom="10dp"
                        android:text="Deskripsi" />

                    <TextView
                        android:id="@+id/homedetailprofileDeskripsi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:text="asdl;fk kjf;ladsjdl;fkj;asdlkfjaajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;dlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;" />
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>



</android.support.v4.widget.NestedScrollView>
